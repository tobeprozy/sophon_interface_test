    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) segformer",
            "type": "cppdbg",
            "request": "launch",
            "program": "${fileDirname}/test.pcie",
            // "program": "${fileDirname}/segformer_bmcv.pcie",
            "args": [],
            "cwd":"${fileDirname}",
            "miDebuggerPath": "/usr/bin/gdb",
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "Build",

        }

    ]

}